// Soma.java
public class Soma {
    public static int soma(int a, int b) {
        return a + b;
    }

    public static int subtracao(int a, int b) {
        return a - b;
    }

    public static void main(String[] args) {
        System.out.println("Soma: " + soma(2, 3));
        System.out.println("Subtração: " + subtracao(5, 3));
    }
}
